{
  "name": "kunpeng_front",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build:preview": "run-p type-check build-only:preview",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "build-only:preview": "vite build --mode=preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:js:fix": "eslint src/**/*.js --fix",
    "lint:*ss:fix": "stylelint src/**/*.*ss --fix  --fix",
    "lint:prettier:check": "npx prettier --check .",
    "lint:prettier:format": "npx prettier --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --config commitlint.config.js -e -V"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.10",
    "axios": "^0.27.2",
    "core-js": "^3.8.3",
    "element-plus": "^2.2.11",
    "eslint-plugin-prettier": "^4.2.1",
    "lodash.clonedeep": "^4.5.0",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "store": "^2.0.12",
    "vue": "^3.2.37",
    "vue-ui": "^0.1.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.47",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "mockjs2": "^1.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vue-router": "4",
    "vue-tsc": "^0.38.8"
  }
}
